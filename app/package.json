{
  "name": "wan22-electron",
  "version": "0.1.0",
  "description": "Wan2.2 Electron desktop app",
  "main": "dist-electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cross-env VITE_DEV_SERVER=true vite\" \"cross-env ELECTRON_START_URL=http://localhost:5173 electron .\"",
    "build:renderer": "vite build",
    "build:electron": "node ./scripts/build-electron.mjs",
    "build": "npm run build:renderer && npm run build:electron",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.25.9",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "recharts": "^3.1.2"
  },
  "build": {
    "appId": "com.xx.wan22",
    "files": [
      "dist/**",
      "dist-electron/**",
      "preload/**",
      "renderer/**",
      "main/**",
      "scripts/**"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
